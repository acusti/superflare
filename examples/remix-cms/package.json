{
  "name": "remix-cms",
  "version": "1.0.0",
  "description": "An example Superflare app powered by Remix",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler deploy",
    "dev:remix": "remix vite:dev",
    "dev:superflare": "NODE_ENV=development superflare dev -b APP_KEY=secret",
    "dev": "pnpm build && concurrently \"npm:dev:*\"",
    "start": "wrangler -j dev",
    "migrate": "superflare migrate",
    "typegen": "wrangler -j types",
    "typecheck": "tsc"
  },
  "keywords": [],
  "author": "Josh Larson <jplhomer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@remix-run/cloudflare": "^2",
    "@remix-run/react": "^2",
    "@remix-run/server-runtime": "^2",
    "@remix-run/v1-route-convention": "^0.1.4",
    "@superflare/remix": "workspace:*",
    "@tailwindcss/forms": "^0.5.3",
    "clsx": "^1.2.1",
    "highlight.js": "^11.7.0",
    "isbot": "^3.6.6",
    "marked": "^4.2.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superflare": "workspace:*",
    "textarea-markdown-editor": "^1.0.4",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240718.0",
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^2",
    "@remix-run/eslint-config": "^2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/marked": "^4.0.8",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.4.4",
    "tsconfig": "workspace:*",
    "typescript": "^5",
    "vite": "^5.3.4",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3"
  },
  "engines": {
    "node": ">=16.13"
  }
}
