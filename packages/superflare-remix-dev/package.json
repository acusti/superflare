{
  "name": "@superflare/remix-dev",
  "version": "0.0.1",
  "description": "Vite dev plugin for Superflare with Remix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/jplhomer/superflare/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "typecheck": "tsc"
  },
  "keywords": [
    "remix",
    "vite",
    "cloudflare",
    "superflare"
  ],
  "author": "Josh Larson <jplhomer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240718.0",
    "@remix-run/cloudflare": "^2",
    "@remix-run/dev": "^2",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^5",
    "vite": "^5",
    "superflare": "workspace:*",
    "wrangler": "^3"
  },
  "peerDependencies": {
    "@remix-run/cloudflare": "^2",
    "@remix-run/dev": "^2",
    "superflare": "workspace:*",
    "wrangler": "^3"
  },
  "dependencies": {
    "@superflare/remix": "workspace:*"
  }
}
